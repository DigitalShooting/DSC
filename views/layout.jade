doctype html
html(lang="de")
	head
		title 
			block title
			
		link(href="/css/style.css", rel="stylesheet")
		
		script(src="/js/jquery-2.1.4.min.js")
		script(src="/socket.io/socket.io.js")
		script(src="/js/bootstrap.min.js")
	
		meta(name="viewport", content="width=device-width")
	body
		nav(class="navbar navbar-default")
			div(class="aktuellerSchuss container w20 h100")
				p Aktueller Schuss
				h3(class="value")
				p(class="value2")
			div(class="anzahlShots container w20 h100")
				p Anzahl Schüsse
				h3(class="value")
				p(class="value2")
			div(class="schnitt container w20 h100")
				p Schnitt
				h3(class="value")
				p(class="value2")
			div(class="ringeGesamt container w20 h100")
				p Gesamt
				h3(class="value")
				p(class="value2")
			div(class="disziplin container w20 h100")
				p Disziplin
				h3(class="value")
				p(class="value2")
					

		block body
		nav(class="navbar navbar-default")
			div(class="name container w20 h100")
				p Name
				h3(class="value")
				p Stand 1
			div(class="verein container w20 h100")
				p Verein
				h3(class="value")
				p(class="value2")
			div(class="time container w20 h100")
				p Uhr
				h3(class="value")
				p(class="value2")
			div(class="container w20 h100")
				p Verbleibende Zeit
				h3 09:20 min
				p 50 min
			div(class="menu container w20 h100")
				p Logout/ Disziplin
				h3 Menü
		
		

		div(class="modal fade" id="mainMenu" tabindex="-1" role="dialog" aria-labelledby="myModalLabel")
			div(class="modal-dialog" role="document")
				div(class="modal-content")
					div(class="modal-header")
						button(type="button" class="close" data-dismiss="modal" aria-label="Close")
							span(aria-hidden="true") &times;
						h4(class="modal-title" id="myModalLabel") Menü
					div(class="modal-body")
						div(class="list-group")
							p(class="actionNewTarget list-group-item") Neue Scheibe
							p(class="actionMatch list-group-item") Match
					div(class="modal-footer")
						button(type="button" class="btn btn-default" data-dismiss="modal") Close
		
		
		
		script(src="/js/modules.js")
		script(src="/js/index.js")
